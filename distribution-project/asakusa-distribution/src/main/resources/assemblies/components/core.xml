<component xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/component/1.1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/component/1.1.2 http://maven.apache.org/xsd/component-1.1.2.xsd">

  <dependencySets>
    <dependencySet>
      <scope>test</scope>
      <useProjectArtifact>false</useProjectArtifact>
      <outputDirectory>/</outputDirectory>
      <outputFileNameMapping></outputFileNameMapping>
      <includes>
        <include>com.asakusafw:asakusa-runtime-configuration:jar:dist</include>
      </includes>
      <unpack>true</unpack>
      <unpackOptions>
        <excludes>
          <exclude>META-INF/</exclude>
        </excludes>
        <lineEnding>unix</lineEnding>
      </unpackOptions>
      <fileMode>0644</fileMode>
      <directoryMode>0755</directoryMode>
    </dependencySet>
    <dependencySet>
      <scope>test</scope>
      <useProjectArtifact>false</useProjectArtifact>
      <outputDirectory>core/lib</outputDirectory>
      <includes>
        <include>com.asakusafw:asakusa-runtime-all:jar:lib</include>
      </includes>
      <outputFileNameMapping>${artifact.artifactId}.${artifact.extension}</outputFileNameMapping>
      <fileMode>0644</fileMode>
      <directoryMode>0755</directoryMode>
    </dependencySet>
  </dependencySets>

</component>